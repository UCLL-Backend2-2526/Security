meta {
  name: Log in as editor@example.com
  type: http
  seq: 1
}

post {
  url: {{host}}/api/v1/sessions
  body: json
  auth: none
}

body:json {
  {
    "emailAddress": "editor@example.com",
    "password": "editor"
  }
}

script:post-response {
  bru.setVar('editor_jwt', res.getBody().token);
}

settings {
  encodeUrl: true
  timeout: 0
}
